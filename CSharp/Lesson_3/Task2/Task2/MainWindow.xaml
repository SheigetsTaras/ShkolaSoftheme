        <Label x:Name="label1" Content="Mounth" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,69,0,0"/>
        <Label x:Name="label2" Content="Year" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,128,0,0"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="190,13,0,0"/>
        <Label x:Name="label3" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="190,118,0,0"/>
        <Label x:Name="label4" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="190,144,0,0"/>
        <Label x:Name="label5" Content="" HorizontalAlignment="Left" Margin="190,170,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Calculate" HorizontalAlignment="Left" Margin="10,187,0,0" VerticalAlignment="Top" Width="75" Height="22" Click="button_Click"/>
        <Label x:Name="label6" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,214,0,0"/>

    </Grid>
</Window>
